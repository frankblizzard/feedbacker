<% title "Project" %>

<p>
  <strong>Name:</strong>
  <%= @project.name %>
</p>
<p>
  <strong>Started At:</strong>
  <%= @project.started_at %>
</p>
<p>
  <strong>Ends At:</strong>
  <%= @project.ends_at %>
</p>
<p>
  <strong>Description:</strong>
  <%= @project.description %>
</p>

<p>
  <%= link_to "Edit", edit_project_path(@project) %> |
  <%= link_to "Destroy", @project, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", projects_path %>
</p>
