<%= form_for @project do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :project_nr %><br />
    <%= f.text_field :project_nr %>
  </p> 
 <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :started_at %><br />
    <%= f.date_select :started_at %>
  </p>
  <p>
    <%= f.label :ends_at %><br />
    <%= f.date_select :ends_at %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description, :class => "mceEditor" %>
  </p>
  <p>
    <%= f.label :user_tokens, "Involved" %><br />
    <%= f.text_field :user_tokens,  "data-pre" =>  @project.users.map(&:attributes).to_json %>
  </p>
  <p><%= f.submit %></p>
<% end %>
